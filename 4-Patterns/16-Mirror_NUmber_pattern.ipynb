   "source": [
    "# Mirror Number pattern\n",
    "for n=4\n",
    "   1\n",
    "  12\n",
    " 123\n",
    "1234"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "775ed8ee",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n",
      "   1\n",
      "  12\n",
      " 123\n",
      "1234\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "i=1\n",
    "while(i<=n):\n",
    "    j=1\n",
    "    k=1\n",
    "    while(j<=n):\n",
    "        if(j<=n-i):\n",
    "            print(' ',end='')\n",
    "        else:\n",
    "            print(k,end='')\n",
    "            k+=1\n",
    "        j+=1\n",
    "    print()\n",
    "    i+=1"
   ]
  },
