   "source": [
    "# Print Star Pattern\n",
    "Pattern for N = 4\n",
    "\n",
    "   *\n",
    "  ***\n",
    " *****\n",
    "*******"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7f25c511",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n",
      "   *\n",
      "  ***\n",
      " *****\n",
      "*******\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "i=1\n",
    "p=1\n",
    "while(i<=n):\n",
    "    j=1\n",
    "    k=1\n",
    "    while(k<=n-i):\n",
    "        print(' ',end='')\n",
    "        k+=1\n",
    "    while(j<=p):\n",
    "        print('*',end='')\n",
    "        j+=1\n",
    "    print()\n",
    "    p+=2\n",
    "    i+=1\n",
    "    "
   ]
