     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n",
      "****\n",
      "***\n",
      "**\n",
      "*\n"
     ]
    }
   ],
   "source": [
    "n = int(input())\n",
    "i=1\n",
    "while(i<=4):\n",
    "    j=1\n",
    "    while(j<=n-i+1):\n",
    "        print(\"*\", end='')\n",
    "        j+=1\n",
    "    print()\n",
    "    i+=1"
   ]
