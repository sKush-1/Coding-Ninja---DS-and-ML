   "source": [
    "n = int(input())\n",
    "i=1\n",
    "while(i<=n):\n",
    "    a = chr(ord('A')+i-1)\n",
    "    j=1\n",
    "    while(j<=i):\n",
    "        print(a, end='')\n",
    "        j+=1\n",
    "    print()\n",
    "    i+=1\n",
    "    "
