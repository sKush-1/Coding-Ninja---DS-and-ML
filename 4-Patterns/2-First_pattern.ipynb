   "source": [
    "n = int(input())\n",
    "i = 1\n",
    "while(i<=n):\n",
    "    j=1\n",
    "    while(j<=n):\n",
    "        print(\"*\", end = ' ')\n",
    "        j+=1\n",
    "    print()\n",
    "    i+=1\n"
